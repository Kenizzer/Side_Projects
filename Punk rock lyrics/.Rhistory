all_bands[sapply(all_bands, is.character)] <- lapply(all_bands[sapply(all_bands, is.character)], as.factor)
all_bands$ORIGIN_YEAR <- as.factor(all_bands$ORIGIN_YEAR)
colnames(all_bands)[1] <- "BAND_NAME"
